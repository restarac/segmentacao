<!DOCTYPE html>
<html>
    <head>
        <title>Segmentacao</title>
        <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
        <%= csrf_meta_tags %>
    </head>

    <body>

        <div id="wrapper" class="active">  
            <!-- Sidebar -->
            <div id="sidebar-wrapper">
                <ul id="sidebar_menu" class="sidebar-nav">
                   <li class="sidebar-brand"><a id="menu-toggle" href="#">Menu<span id="main_icon" class="glyphicon glyphicon-align-justify"></span></a></li>
                </ul>
                <ul class="sidebar-nav" id="sidebar">
                  <li><a href="<%= contacts_url %>">Contacts<span class="sub_icon glyphicon glyphicon-link"></span></a></li>
                   <ul class="sidebar-nav" id="sidebar">
                        <li><a href="<%= segmentations_url %>">Build<span class="sub_icon glyphicon glyphicon-link"></span></a></li>
                        <li><a href="<%= segmentation_workspace_url %>">Run<span class="sub_icon glyphicon glyphicon-link"></span></a></li>
                   </ul>
                </ul>
              </div>
                  
              <!-- Page content -->
              <div id="page-content-wrapper">
                <div class="page-content inset">
                    <%= yield %>
                </div>
            </div>
        </div>
        <%= javascript_include_tag "jquery_ujs" %>
        <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
    </body>
</html>
